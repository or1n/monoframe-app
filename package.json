{
  "name": "monoframe",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:lan": "next dev -H 0.0.0.0",
    "dev:brave": "next dev & sleep 2 && (brave-browser http://localhost:3000 || brave http://localhost:3000 || open -a 'Brave Browser' http://localhost:3000 || echo 'Please open http://localhost:3000 in Brave Browser manually')",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "lint:fix": "eslint --fix .",
    "audit": "npm audit --audit-level=moderate",
    "audit:fix": "npm audit fix",
    "deps:check": "npx npm-check-updates",
    "deps:upgrade": "npx npm-check-updates -u && npm install",
    "upgrade:minor": "npx npm-check-updates -u --target minor && npm install",
    "check": "npm run lint && npm run audit"
  },
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 Firefox versions",
    "last 2 Safari versions",
    ">= 0.5%",
    "not dead",
    "not op_mini all"
  ],
  "dependencies": {
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.26",
    "lucide-react": "^0.562.0",
    "next": "16.1.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.1.0",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
